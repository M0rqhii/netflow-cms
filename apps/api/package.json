{
    "name": "@repo/api",
    "version": "1.0.0",
    "private": true,
    "description": "Backend API - NestJS + Prisma",
    "scripts": {
        "dev": "node scripts/dev-with-link.js",
        "prebuild": "rm -rf dist 2>/dev/null || true",
        "build": "nest build",
        "postbuild": "cd dist && (ln -sf apps/api/src/main.js main.js 2>/dev/null || ln -sf src/main.js main.js 2>/dev/null || true)",
        "watch:link": "while true; do sleep 2; ([ -f dist/apps/api/src/main.js ] && [ ! -f dist/main.js ] && cd dist && ln -sf apps/api/src/main.js main.js 2>/dev/null) || ([ -f dist/src/main.js ] && [ ! -f dist/main.js ] && cd dist && ln -sf src/main.js main.js 2>/dev/null) || true; done",
        "start": "node dist/apps/api/src/main.js",
        "start:prod": "node dist/apps/api/src/main.js",
        "clean": "rm -rf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:cov:check": "node test/coverage-check.js",
        "test:all": "npm run test && npm run test:e2e",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "type-check": "tsc --noEmit",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:studio": "prisma studio",
        "db:seed": "ts-node prisma/seed.ts",
        "db:seed:demo": "ts-node scripts/seed-demo-org.ts",
        "assign-org-owner": "ts-node scripts/assign-org-owner.ts",
        "assign-org-owners-all": "ts-node scripts/assign-org-owners-all.ts",
        "create-super-admin": "ts-node scripts/create-super-admin.ts",
        "setup-super-admin": "ts-node scripts/setup-super-admin.ts"
    },
    "dependencies": {
        "@nestjs/cache-manager": "^2.1.1",
        "@nestjs/common": "^10.3.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.3.0",
        "@nestjs/jwt": "^11.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^10.3.0",
        "@nestjs/swagger": "^11.2.4",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^3.1.0",
        "@prisma/client": "^5.7.0",
        "@repo/schemas": "workspace:*",
        "bcrypt": "^6.0.0",
        "cache-manager": "^5.3.2",
        "cache-manager-redis-yet": "^5.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.3",
        "express": "^5.2.1",
        "helmet": "^8.1.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "prom-client": "^15.1.3",
        "redis": "^5.10.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "swagger-ui-express": "^5.0.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.2.1",
        "@nestjs/schematics": "^10.0.3",
        "@nestjs/testing": "^10.3.0",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/bcrypt": "^6.0.0",
        "@types/cache-manager": "^4.0.3",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^29.5.11",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.0",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prisma": "^5.7.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.0"
    }
}
