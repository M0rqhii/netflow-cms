{
    "name":  "@repo/api",
    "version":  "1.0.0",
    "private":  true,
    "description":  "Backend API - NestJS + Prisma",
    "scripts":  {
                    "dev":  "nest start --watch",
                    "build":  "nest build",
                    "start":  "node dist/main",
                    "start:prod":  "node dist/main",
                    "test":  "jest",
                    "test:watch":  "jest --watch",
                    "test:coverage":  "jest --coverage",
                    "test:cov:check":  "node test/coverage-check.js",
                    "test:all":  "npm run test \u0026\u0026 npm run test:e2e",
                    "test:e2e":  "jest --config ./test/jest-e2e.json",
                    "lint":  "eslint \"{src,test}/**/*.ts\"",
                    "type-check":  "tsc --noEmit",
                    "db:generate":  "prisma generate",
                    "db:migrate":  "prisma migrate dev",
                    "db:migrate:deploy":  "prisma migrate deploy",
                    "db:studio":  "prisma studio",
                    "db:seed":  "ts-node prisma/seed.ts"
                },
    "dependencies":  {
                         "@nestjs/cache-manager":  "^2.1.1",
                         "@nestjs/common":  "^10.3.0",
                         "@nestjs/config":  "^3.1.1",
                         "@nestjs/core":  "^10.3.0",
                         "@nestjs/jwt":  "^11.0.1",
                         "@nestjs/passport":  "^11.0.5",
                         "@nestjs/platform-express":  "^10.3.0",
                         "@prisma/client":  "^5.7.0",
                         "@repo/schemas":  "workspace:*",
                         "bcrypt":  "^6.0.0",
                         "cache-manager":  "^5.3.2",
                         "cache-manager-redis-yet":  "^5.1.2",
                         "passport":  "^0.7.0",
                         "passport-jwt":  "^4.0.1",
                         "passport-local":  "^1.0.0",
                         "reflect-metadata":  "^0.1.13",
                         "rxjs":  "^7.8.1",
                         "zod":  "^3.22.4",
                         "@nestjs/throttler":  "^3.1.0"
                     },
    "devDependencies":  {
                            "@nestjs/cli":  "^10.2.1",
                            "@nestjs/schematics":  "^10.0.3",
                            "@nestjs/testing":  "^10.3.0",
                            "@types/bcrypt":  "^6.0.0",
                            "@types/cache-manager":  "^4.0.3",
                            "@types/express":  "^4.17.21",
                            "@types/jest":  "^29.5.11",
                            "@types/node":  "^20.10.0",
                            "@types/passport-jwt":  "^4.0.1",
                            "@types/passport-local":  "^1.0.38",
                            "@types/supertest":  "^6.0.3",
                            "@typescript-eslint/eslint-plugin":  "^6.15.0",
                            "@typescript-eslint/parser":  "^6.15.0",
                            "eslint":  "^8.56.0",
                            "jest":  "^29.7.0",
                            "prisma":  "^5.7.0",
                            "supertest":  "^6.3.3",
                            "ts-jest":  "^29.1.1",
                            "ts-node":  "^10.9.2",
                            "typescript":  "^5.3.0"
                        }
}
